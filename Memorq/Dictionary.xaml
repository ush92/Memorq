<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Common -->
    <system:String x:Key="Name">Nazwa</system:String>
    <system:String x:Key="Question">Pytanie</system:String>
    <system:String x:Key="Answer">Odpowiedź</system:String>
    <system:String x:Key="Quit">Zamknij</system:String>
    <system:String x:Key="Cancel">Anuluj</system:String>

    <!-- Category Manager -->
    <system:String x:Key="ChooseCategory">Wybierz kategorię</system:String>
    <system:String x:Key="NewCategory">Nowa kategoria</system:String>
    <system:String x:Key="ChangeCategoryName">Zmień nazwę kat.</system:String>
    <system:String x:Key="DeleteCategory">Usuń kategorię</system:String>
    <system:String x:Key="EditItem">Edytuj pyt./odp.</system:String>
    <system:String x:Key="DeleteItem">Usuń pyt./odp.</system:String>

    <system:String x:Key="ResetItem">Resetuj współczynniki nauki</system:String>
    <system:String x:Key="SwapQuestionAnswer">Zamień pyt./odp.</system:String>

    <!-- Right Panel buttons -->
    <system:String x:Key="AddBtn_right">Dodaj</system:String>
    <system:String x:Key="NewBtn_right">Nowe</system:String>
    <system:String x:Key="ForceBtn_right">Wymuś</system:String>

    
    <!-- Main Menu -->
    <system:String x:Key="Menu_File">Plik</system:String>
    <system:String x:Key="Menu_File_Manager">Menedżer kategorii</system:String>
    <system:String x:Key="Menu_File_ImportExport">Import/Eksport</system:String>
    <system:String x:Key="Menu_File_Exit">Wyjście</system:String>

    <system:String x:Key="Menu_Edit">Edycja</system:String>
    
    <system:String x:Key="Menu_Stats">Statystyki</system:String>
    
    <system:String x:Key="Menu_Options">Opcje</system:String>
    
    <system:String x:Key="Menu_Help">Pomoc</system:String>
    <system:String x:Key="Menu_Help_About">O programie</system:String>
    
    
    <!-- Messages -->
    <system:String x:Key="MsgCategoryDuplicate">Kategoria o podanej nazwie istnieje już w bazie kategorii</system:String>
    <system:String x:Key="MsgCategoryNoName">Podaj nazwę kategorii</system:String>
    <system:String x:Key="MsgCategoryNotChosen">Wybierz kategorię</system:String>
    <system:String x:Key="MsgEnterNameOfNewCategory">Podaj nazwę kategorii</system:String>
    <system:String x:Key="MsgCategoryDelete">Czy na pewno chcesz usunąć tę kategorię?</system:String>
    <system:String x:Key="MsgItemDelete">Czy na pewno chcesz usunąć to pytanie/odpowiedź?</system:String>
    
</ResourceDictionary>